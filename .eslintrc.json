{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "airbnb-base"
  ],
  "rules": {
    "no-console": ["error", { "allow": ["warn", "error", "info"] }],
    "import/extensions": "off",
    "import/prefer-default-export": "off",
    "semi": ["error", "always"]
  }
}
